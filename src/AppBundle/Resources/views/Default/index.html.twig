    <script id="tpl_page_search" type="text/template">
      <div id="page_search">
        <div class="search-result" id="people-search-result">
          <div class="people-search-result-inner row">
            <form class="form dbperson-search-form col-md-4">
              <div class="source-type">
                <h4>Choose your Lead Source</h4><img src="/images/company-gray.png"/>
                <div class="total-count-container invisible"><span class="total-count"></span> leads</div>
              </div>
              <div class="form-inner">
                <h4 class="form-section">Company/People</h4>
                <div class="company-people-filter-panes filter-panes">
                  <div class="filter-pane">
                    <div class="form-group">
                      <input class="form-control" type="text" name="person_name" placeholder="Person:"/>
                    </div>
                    <div class="form-group">
                      <input class="form-control" type="text" name="company_names" placeholder="Company:"/>
                    </div>
                    <div class="form-group">
                      <input class="form-control" type="text" name="person_title" placeholder="Job Title:"/>
                    </div>
                    <div class="form-group">
                      <div class="form-control role-list-container dbperson-list-container">
                        <div class="form-control-static">Role:</div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="form-control seniority-list-container dbperson-list-container">
                        <div class="form-control-static">Seniority:</div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="form-control industry-list-container dbperson-list-container">
                        <div class="form-control-static">Industry:</div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="form-control size-list-container dbperson-list-container">
                        <div class="form-control-static">Size:</div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="form-control revenue-list-container dbperson-list-container">
                        <div class="form-control-static">Revenue:</div>
                      </div>
                    </div>
                  </div>
                </div>
                <h4 class="form-section">Location</h4>
                <div class="location-filter-panes filter-panes">
                  <div class="filter-pane">
                    <div class="form-group">
                      <div class="form-control country-list-container dbperson-list-container">
                        <div class="form-control-static">Location:</div>
                      </div>
                    </div>
                    <div class="form-group location-input-container">
                      <label> - OR - </label>
                      <input class="form-control input-inline" type="text" name="location" placeholder="City/State/ZIP/Country:"/>
                    </div>
                    <ul class="location-match-type-list">
                      <li>
                        <input id="search-location-match-type-person-or-hq" type="radio" name="location_match_type" value="PersonOrHQ"/>
                        <label for="search-location-match-type-person-or-hq">Either the Person or Company HQ is at selected location</label>
                      </li>
                      <li>
                        <input id="search-location-match-type-person-and-hq" type="radio" name="location_match_type" value="PersonAndHQ"/>
                        <label for="search-location-match-type-person-and-hq">Both the Person or Company HQ are at selected location</label>
                      </li>
                      <li>
                        <input id="search-location-match-type-person" type="radio" name="location_match_type" value="Person"/>
                        <label for="search-location-match-type-person">Person is at selected location</label>
                      </li>
                      <li>
                        <input id="search-location-match-type-hq" type="radio" name="location_match_type" value="HQ"/>
                        <label for="search-location-match-type-hq">Company HQ is at selected location</label>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </form>
            <div class="search-result col-md-8">
